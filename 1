 settings:
  env:
    BASE_DIR: E:\Taurus  # 脚本目录
  artifacts-dir: E:\Taurus\artifacts\simple1\%Y-%m-%d_%H-%M  # 保存文件路径

execution:
- concurrency: 10  # 并发线程数
  steps: 5         # 阶梯
  ramp-up: 1m      # 启动时间
  hold-for: 2m30s  # 持续时间
  scenario: simple1  # 场景名称
  system-properties:
    jmeter.save.saveservice.interval: 1000  # 设置采样间隔为1000ms，即1秒

scenarios:
  simple1:
    script: '${BASE_DIR}/build-web-test-plan.jmx'  # JMeter脚本路径
    listeners:
      - jp@gc - Transactions per Second

modules:
  jmeter:
    download-link: 'https://mirrors.tuna.tsinghua.edu.cn/apache//jmeter/binaries/apache-jmeter-{version}.zip' # 下载地址
    version: 5.2.1  # JMeter版本号

reporting:
- module: final-stats   # 摘要报告
- module: console       # 控制台
